<% provide(:title, 'Всі користувачі') %>
<div class="users">
<h1>Всі користувачі</h1>
    <%= form_for users_path, method: :get do %>
  <div class="search">
    <div class="input">
      <%= text_field_tag :search, params[:search], placeholder: "Пошук...", class: 'form-control' %>
    </div>
    <div class="button">
      <%= button_tag('Шукати',name: nil, class: 'btn btn-info')%>
    </div>
  </div>
    <% end %>
  <ul class="items">
    <%= render @users%>
  </ul>
<%= will_paginate%>
</div>